{
  "compilerOptions": {
    "target": "ES2019",
    "useDefineForClassFields": true,
    "module": "CommonJS",
    "lib": [
      "ES2019",
      "WebWorker"
    ],
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "include": [
    "src"
  ]
}